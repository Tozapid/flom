AT_BANNER([Transactional sequence resources use case checks])

AT_SETUP([Use case YY])
AT_DATA([expout],
[[ 1 locking for 3 seconds
 2 locking for 3 seconds
 3 locking for 3 seconds
0
 1 ending
1
 2 ending
2
 3 ending
]])
AT_CHECK([pkill flom], [ignore], [ignore], [ignore])
AT_CHECK([flom_test_exec4.sh 1 0 3 "-r _s_a[[3]]" & flom_test_exec4.sh 2 1 3 "-r _s_a[[3]]" & flom_test_exec4.sh 3 2 3 "-r _s_a[[3]]"], [0], [expout], [ignore])
